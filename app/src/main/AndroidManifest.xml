<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bicpark">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bicpark">
        <activity android:name=".Activitys.PlantaActivity"></activity>
        <activity android:name=".Activitys.EdificioActivity" />
        <activity android:name=".Activitys.PlazaActivity" />
        <activity android:name=".Activitys.EmpresaActivity" />
        <activity android:name=".Activitys.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.InfoActivity" />
        <activity android:name=".Activitys.MainActivity" />
    </application>

</manifest>